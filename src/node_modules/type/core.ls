_typeof = (value) -> typeof! value
_is = (type, value) --> _typeof(value) is type

export {
	typeof: _typeof

	is:         _is

	isNumber:   _is 'Number'
	isBoolean:  _is 'Boolean'
	isString:   _is 'String'
	isRegExp:   _is 'RegExp'
	isDate:     _is 'Date'
	isArray:    _is 'Array'
	isObject:   _is 'Object'
	isFunction: _is 'Function'
}
