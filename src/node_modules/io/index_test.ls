require! {
# ---------------------------------------------------------------------- Own STD
	'tester'
# ------------------------------------------------------------------------ Input
	io: './'
}

{Logger} = tester.logger
$logger = Logger!


# Rendering --------------------------------------------------------------------

content = {
	'Section 1': [
		'My 1st paragraph'
		[
			'A'
			[
				'super'
				'nested'
			]
			'list'
		]
	]
	'Section 2':
		'Section 2.1': [
			'My 2nd paragraph'
		]
}

# Markdown ---------------------------------------------------------------------

$logger.separator 'Markdown'
$logger.on!

$logger.log content
$logger.delimiter!
$logger.logStr io.render 'md' content
$logger.delimiter!

# MediaWiki --------------------------------------------------------------------

$logger.separator 'Markdown'
$logger.on!

$logger.log content
$logger.delimiter!
$logger.logStr io.render 'mw' content
$logger.delimiter!
