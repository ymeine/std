require! {
	moment

	tester

	date: './'
}

{Logger} = tester.logger
$logger = Logger!



# Export -----------------------------------------------------------------------

$logger.separator 'Export'
$logger.on!

data = moment!
$logger.log data
exported = date.export data
$logger.log exported

# Import -----------------------------------------------------------------------

$logger.separator 'Import'
$logger.on!

$logger.log date.import exported

# Convert ----------------------------------------------------------------------

$logger.separator 'Convert'
$logger.on!
