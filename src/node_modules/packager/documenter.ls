require! {
	packager: './core'
}



getAPI = (exports) ->
	spec = exports.__module__.__spec__

	{
		values
		submodules
	} = spec

	{
		values: for value in values
			{api} = value
			api <<< value{names}
			api
		submodules: for submodule in submodules
			{api} = submodule
			api <<< submodule{names}
			api
	}



packager.export {
	module

	values: [
		{
			value: getAPI
			names: <[
				getAPI
				api
				API
			]>
		}
	]
}
