require! {
	tester

	packager: './'
}

{Logger} = tester.logger
$logger = new Logger!




$logger.separator 'Module'
$logger.on!

mod = require './_test/mod'
$logger.log mod
$logger.log mod.sub1
$logger.log mod.submod1
$logger.log mod.sub2
$logger.log mod.submod2

# $logger.stop!

# ------------------------------------------------------------------------------

$logger.separator 'Export values'
$logger.on!

packager.export {
	module
	values: [
		{
			value: 1
			names: <[
				1
				one
				I
			]>
		}
		{
			value: 5
			names: <[
				5
				five
				V
			]>
		}
	]
}
$logger.log module.exports

# ------------------------------------------------------------------------------

$logger.separator 'Export submodules'
$logger.on!

packager.export {
	module
	submodules: [
		{
			path: '_test/submodule1'
			names: <[
				sub1
				submod1
			]>
		}
		{
			path: '_test/submodule2'
			names: <[
				sub2
				submod2
			]>
		}
	]
}
$logger.log module.exports

# ------------------------------------------------------------------------------

$logger.separator 'Spec & doc'
$logger.on!

$logger.log module.__spec__
$logger.log module.exports.__module__.__spec__
